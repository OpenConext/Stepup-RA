{
    "name": "surfnet/stepup-ra",
    "license": "Apache-2.0",
    "description": "The SURFnet Step-up Registration Authority application allows Registration Authorities to vet identities by combining a real-world identity, federated identity and a token.",
    "autoload": {
        "psr-4": {
            "Surfnet\\": "src\\Surfnet"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "package": "ee/dataexporter-bundle",
            "type": "vcs",
            "url":  "https://github.com/MKodde/DataExporter.git"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "fortawesome/font-awesome": "~4.2.0",
        "guzzlehttp/guzzle": "^6",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/translation-bundle": "~1.3.0",
        "knplabs/knp-paginator-bundle": "~2.4",
        "mopa/bootstrap-bundle": "^3.2",
        "mopa/composer-bridge": "~1.5",
        "nelmio/security-bundle": "~1.6",
        "openconext/monitor-bundle": "^1.0",
        "ramsey/uuid": "^3.5",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "~3.0",
        "surfnet/stepup-bundle": "^4.0",
        "surfnet/stepup-middleware-client-bundle": "^3.0",
        "surfnet/stepup-saml-bundle": "^4.1",
        "surfnet/stepup-u2f-bundle": "dev-develop",
        "symfony/monolog-bundle": "^3.5",
        "symfony/symfony": "3.4.*",
        "twbs/bootstrap": "~3.3.0",
        "twig/extensions": "~1.5",
        "twig/twig": "1.41.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^2.2",
        "mockery/mockery": "~1.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^5.7",
        "sebastian/exporter": "~2.0",
        "sebastian/phpcpd": "^2.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "5.6"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "symfony-app-dir": "app",
        "symfony-var-dir": "app",
        "symfony-bin-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": [
            {
                "file": "app/config/parameters.yml"
            },
            {
                "file": "app/config/samlstepupproviders_parameters.yml"
            },
            {
                "file": "app/config/global_view_parameters.yml"
            }
        ]
    }
}
